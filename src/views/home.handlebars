<table class="data-table">
    <thead>
        <tr>
            <th>Nodo</th>
            <th>ip</th>
            <th>Technology</th>
            <th>Availability</th>
            <th>Frequency</th>
            <th>Channel</th>
            <th>Count SM</th>
            <th>MB</th>
            <th>Service Max</th>
        </tr>
    </thead>
    <tbody>
        {{#each resGet}}
        <tr>
            <td>{{this.node}}</td>
            <td>{{this.ip}}</td>
            <td>{{this.technology}}</td>
            <td>{{this.availability}}</td>
            <td>{{this.frequency}}</td>
            <td>{{this.channel}}</td>
            <td>{{this.countSM}}</td>
            <td>{{this.MB}}</td>
            <td>{{this.serviceMax}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>